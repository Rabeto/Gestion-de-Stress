option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: "Webproject.settings"
    PYTHONPATH: "/var/app/current:$PYTHONPATH"
  aws:elasticbeanstalk:container:python:
    WSGIPath: "Webproject.wsgi:application"
  aws:elasticbeanstalk:environment:proxy:staticfiles:
    /static: static
  
container_commands:
  collectstatic:
    command: "source /var/app/venv/*/bin/activate && python3 manage.py collectstatic"
  01_makemigrations:
    command: "source /var/app/venv/*/bin/activate && python3 manage.py makemigrations"
    leader_only: true
  02_migrate:
    command: "source /var/app/venv/*/bin/activate && python3 manage.py migrate"
    leader_only: true
  03_createuser:
    command: "source /var/app/venv/*/bin/activate && python3 manage.py createsuperuser --username admin --password admin --email foo@foo.foo"
    leader_only: true
  04_loaddata:
   command: "source /var/app/venv/*/bin/activate && python3 manage.py loaddata ./data.json"
    leader_only: true


  
  

